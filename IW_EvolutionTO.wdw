     name : FillChart
     internal_properties : CAAAAAgAAADeb/Foj6TUQZJgtYwLETewgPv290JpfSBNLbQw/OkoveoRcZtb9SXw7Sz9XSQIlj/YiXWSCjrStQBmK0/6vXhzbiRYZTs55OOc132y1g0Gm4j10AvUPwjZy6Pwt9aN7iVw3UsKdVoDGo+S+uEF
     procedure_id : 2015370192139463448
     type_code : 14
     code : |1-
      // Summary: Fill the chart with the turnover of the last 12 months (and the comparison with year N-1)
      // Syntax:
      //FillChart ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      Procédure FillChart()
      
      dDate is date 
      
      for i = 1 to 12
      	
      	QRY_TOMonth.paramMonth = Left(dDate,6)
      	HExecuteQuery(QRY_TOMonth)
      	HReadFirst(QRY_TOMonth)	
      	grAddData(CHART_EvolutionTO, 1, 12-i, QRY_TOMonth.the_sum_TotalIOT)
      	
      	//And the previous year
      	dDate..Year--
      	QRY_TOMonth.paramMonth = Left(dDate,6)
      	HExecuteQuery(QRY_TOMonth)
      	HReadFirst(QRY_TOMonth)	
      	grAddData(CHART_EvolutionTO, 2, 12-i, QRY_TOMonth.the_sum_TotalIOT)
      		
      	grCategoryLabel(CHART_EvolutionTO, 12-i, DateToString(dDate,"Mmm"))
      	
      	dDate..Year++
      	dDate..Month --
      	
      end
      
      grDraw(CHART_EvolutionTO)
     type : 458752
