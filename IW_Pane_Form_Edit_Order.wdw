                    p_codes :
                     -
                       code : |1+
                        //If it is a new order/estimaate >> process for creating an order record with a generated number
                        if gnOrderID = -1 then 
                        	
                        	OrderCreate()
                        	Orders.CustomerUUID = gCustomerUUID
                        	
                        else
                        	
                        	//If it is an estimate or an order that is modified
                        	//Delete the delivery or invoicing addresses if they are modified in order to re-create them
                        	
                        	nDeliveryOrderID is int = Orders.DeliveryAddressID 
                        	
                        	if GR_DeliveryAddress..Modified = True then
                        		
                        		Orders.DeliveryAddressID  = 0 
                        		HModify(Orders)
                        		
                        		HReadSeekFirst(ArchiveAddress,AddressID, nDeliveryOrderID)
                        		if HFound(ArchiveAddress) then
                        			HDelete(ArchiveAddress)
                        		end
                        	end
                        	
                        	if CBOX_IdenticalInvoicingAddress..Modified = True or GR_InvoicingAddress..Modified = True then
                        		nInvoicingOrderID is int = Orders.InvoicingAddressID 
                        		Orders.InvoicingAddressID  = 0 
                        		HModify(Orders)
                        		
                        		if nDeliveryOrderID <> nInvoicingOrderID then
                        			
                        			HReadSeekFirst(ArchiveAddress,AddressID, Orders.InvoicingAddressID )
                        			if HFound(ArchiveAddress) then
                        				HDelete(ArchiveAddress)
                        			end
                        		end
                        	end
                        	
                        	//Delete the order lines if they have been modified in order to re-create them
                        	if TABLE_OrdLine..Modified = True then
                        		for each ordline where OrderID = Orders.OrderID
                        			HDelete(ordline)
                        		end
                        	end
                        	
                        end
                        
                        
                        ScreenToFile()
                        
                        
                        //If new order/new estimate or if the order lines have been modified 
                        if gnOrderID = -1 or TABLE_OrdLine..Modified = True then
                        	
                        	//Create them
                        	
                        	FOR ALL ROW of TABLE_OrdLine
                        		
                        		// Find the product and its tax rate
                        		if HReadSeekFirst(ProductVariation,Reference, COL_Reference) = False then
                        			Error(StringBuild("Unknown %1 reference",COL_Reference))
                        			return
                        		end
                        		if HReadSeekFirst(Product,ProductID,ProductVariation.ProductID) = False or 
                        			HReadSeekFirst(TaxRate,TaxRateID,Product.TaxRateID) = False then
                        			Error(StringBuild("Tax rate unknown for the %1 product", COL_Reference))
                        			return
                        		end
                        		
                        		// Create the order line
                        		HReset(ordline)
                        		ordline.ProductCaption = ProductVariation.VariationCaption
                        		ordline.Quantity = COL_Quantity
                        		ordline.UnitPriceBT = ProductVariation.UnitPriceBT
                        		ordline.TaxRate = TaxRate.Rate
                        		ordline.LineTotal = ordline.UnitPriceBT * ordline.Quantity
                        		ordline.OrderID = Orders.OrderID
                        		ordline.ProductVariationID = ProductVariation.ProductVariationID
                        		if HAdd(ordline)=False then
                        			Error(HErrorInfo())
                        		end	
                        		
                        	end
                        end
                        
                        if gnOrderID = -1 or GR_DeliveryAddress..Modified = True then
                        	
                        	//Shipping address
                        	ArchiveAddress.FullName = EDT_AddressFullName
                        	ArchiveAddress.Company = EDT_CompanyAddress
                        	ArchiveAddress.Way = EDT_AddressStreet
                        	ArchiveAddress.Complement = EDT_AddressComplement
                        	ArchiveAddress.LocalityPOB = EDT_AddressPostalBox
                        	ArchiveAddress.ZipCode = EDT_AddressZipCode
                        	ArchiveAddress.City = EDT_AddressCity
                        	ArchiveAddress.StateProvince = EDT_AddressStateProvince
                        	ArchiveAddress.Country = COMBO_AddressCountry[COMBO_AddressCountry]
                        	
                        	if HAdd(ArchiveAddress) = True then
                        		Orders.DeliveryAddressID = ArchiveAddress.AddressID
                        	end
                        end
                        
                        
                        if gnOrderID = -1 or CBOX_IdenticalInvoicingAddress..Modified = True or GR_InvoicingAddress..Modified = True then
                        	
                        	if CBOX_IdenticalInvoicingAddress = 1 then
                        		Orders.InvoicingAddressID = Orders.DeliveryAddressID
                        	else
                        		
                        		ArchiveAddress.FullName = EDT_InvAddressFullName
                        		ArchiveAddress.Company = EDT_InvAddressCompany
                        		ArchiveAddress.Way = EDT_InvAddressStreet
                        		ArchiveAddress.Complement = EDT_InvAddressComplement
                        		ArchiveAddress.LocalityPOB = EDT_InvAddressPostalBox
                        		ArchiveAddress.ZipCode = EDT_InvAddressZipCode
                        		ArchiveAddress.City = EDT_InvAddressCity
                        		ArchiveAddress.StateProvince = EDT_InvAddressStateProvince
                        		ArchiveAddress.Country = EDT_InvAddressCountry[EDT_InvAddressCountry]
                        		if HAdd(ArchiveAddress) = True then
                        			Orders.InvoicingAddressID = ArchiveAddress.AddressID
                        		end
                        		
                        	end
                        	
                        end
                        
                        if HModify(Orders) = True then
                        	Notify("Add.Document", Orders.OrderID)	
                        	ToastDisplay("Saved")
                        else
                        	Error(HErrorInfo())
                        end
                        
                        // switches the tab to not modified
                        MyInternalWindow..Modified=False
                        
