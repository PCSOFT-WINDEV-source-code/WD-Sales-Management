       name : BTN_Add
       identifier : 0x2c6eb80007660468
       internal_properties : 
       properties :
        x : 1284
        y : 154
        width : 80
        height : 24
       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           code : |1-
            
            // The controls must be valid
            if EDT_Add_Quantity <= 0 then
            	Error("You must enter a valid quantity.")
            	ReturnToCapture(EDT_Add_Quantity)
            end
            HReadSeekFirst(ProductVariation, Reference, EDT_Add_Reference)
            if HFound(ProductVariation) = False then
            	Error("You must enter a valid reference.")
            	ReturnToCapture(EDT_Add_Reference)
            end
            
            // Searches for the product linked to the variation
            HReadSeekFirst(Product, ProductID, ProductVariation.ProductID)
            
            nSubscript is int
            // If the product is already in the table
            nSubscript = TableSeek(TABLE_OrdLine.COL_Reference, EDT_Add_Reference)
            if nSubscript > 0 then
            	
            	// Asks the users what they want to do (replace the quantity, increase the quantity, etc.)
            	switch Dialog(StringBuild("The product #%1 - %2 is already in the order lines.", EDT_Add_Reference, EDT_Add_Caption) +CR+ "What do you want to do ?", ["Replace the quantity", "Increase the quantity", "Cancel"], 1, 3)
            		// Replace the quantity
            		case 1
            			TABLE_OrdLine.COL_Quantity[nSubscript] = EDT_Add_Quantity
            			
            		// Increase the quantity
            		case 2
            			TABLE_OrdLine.COL_Quantity[nSubscript] += EDT_Add_Quantity
            			
            		// Cancel
            		case 3
            			// Clears the controls
            			EDT_Add_Reference = ""
            			EDT_Add_Caption = ""
            			return
            			
            	end	
            	
            else // The product is not yet in the table
            	
            	// Adds a row into the table
            	nSubscript = TableAddLine(TABLE_OrdLine, 0, 0, EDT_Add_Reference)
            	COL_ProdDesc[nSubscript] = ProductVariation.VariationCaption
            	SET_UnitPriceBT[nSubscript] = ProductVariation.UnitPriceBT
            	COL_Quantity[nSubscript] = EDT_Add_Quantity
            	
            	QRY_TaxRateProductVariation.ParamProductVariationID = ProductVariation.ProductVariationID
            	HExecuteQuery(QRY_TaxRateProductVariation)
            	HReadFirst(QRY_TaxRateProductVariation)
            	COL_VATRate[nSubscript] = QRY_TaxRateProductVariation.Rate
            	
            end
            
            // Updates the totals
            SET_SubTotalBT[nSubscript] = COL_Quantity[nSubscript] * SET_UnitPriceBT[nSubscript]
            CalculateTotals()
            
            // Clears the controls
            EDT_Add_Caption = ""
            EDT_Add_Reference = ""
            EDT_Add_Quantity = 0
           type : 18
