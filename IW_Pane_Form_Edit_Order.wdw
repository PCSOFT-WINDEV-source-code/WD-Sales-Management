     p_codes :
      -
        code : |1+
         Procédure MyWindow(gnOrderID is int = -1, gnStatus is int = ORD_WAITINGFORPAYMENT, gCustomerUUID is UUID = Nil_UUID)
         
      -
        code : |1+
         
         //If we are modifying an existing order or estimate, display it
         if gnOrderID <> -1 then 
         	
         	HReadSeekFirst(Orders,OrderID,gnOrderID)
         	if HFound(Orders) then
         		
         		//Display the order information
         		FileToScreen()
         		
         		STC_Total = Orders.Totaltopay
         		
         		//The order status: estimate/order waiting for payment/prepared..
         		gnStatus = Orders.OrderStatus
         		
         		//Client
         		gCustomerUUID = Orders.CustomerUUID	
         		EDT_Customer = FullCustomerName(gCustomerUUID)
         		
         		//The order lines
         		QRY_OrdLinesInOrd.ParamOrderID = gnOrderID
         		HExecuteQuery(QRY_OrdLinesInOrd)
         		for each QRY_OrdLinesInOrd 
         			TableAddLine(TABLE_OrdLine, QRY_OrdLinesInOrd.OrdLineID,gnOrderID, QRY_OrdLinesInOrd.Reference, QRY_OrdLinesInOrd.ProductCaption,  QRY_OrdLinesInOrd.UnitPriceBT,  QRY_OrdLinesInOrd.Quantity, QRY_OrdLinesInOrd.UnitPriceBT * QRY_OrdLinesInOrd.Quantity,QRY_OrdLinesInOrd.TaxRate) 
         		end
         		
         		CalculateTotals()
         		
         		//The delivery and invoicing addresses
         		
         		HReadSeekFirst(ArchiveAddress,AddressID,Orders.DeliveryAddressID)
         		if HFound(ArchiveAddress) then
         			
         			EDT_AddressFullName = ArchiveAddress.FullName
         			EDT_CompanyAddress = ArchiveAddress.Company
         			EDT_AddressStreet = ArchiveAddress.Way
         			EDT_AddressComplement = ArchiveAddress.Complement
         			EDT_AddressPostalBox = ArchiveAddress.LocalityPOB
         			EDT_AddressZipCode = ArchiveAddress.ZipCode
         			EDT_AddressCity = ArchiveAddress.City
         			EDT_AddressStateProvince = ArchiveAddress.StateProvince
         			nCountrySubscript is int = ListSeek(COMBO_AddressCountry,ArchiveAddress.Country)
         			if nCountrySubscript > 0 then ListSelectPlus(COMBO_AddressCountry, nCountrySubscript)
         		end
         		
         		
         		HReadSeekFirst(ArchiveAddress,AddressID,Orders.InvoicingAddressID)
         		if HFound(ArchiveAddress) then
         			//Invoicing address, filled with the customer address beforehand
         			EDT_InvAddressFullName = ArchiveAddress.FullName
         			EDT_InvAddressCompany = ArchiveAddress.Company
         			EDT_InvAddressStreet = ArchiveAddress.Way
         			EDT_InvAddressComplement = ArchiveAddress.Complement
         			EDT_InvAddressPostalBox = ArchiveAddress.LocalityPOB
         			EDT_InvAddressZipCode = ArchiveAddress.ZipCode
         			EDT_InvAddressCity = ArchiveAddress.City
         			EDT_InvAddressStateProvince = ArchiveAddress.StateProvince
         			nCountrySubscript is int = ListSeek(EDT_InvAddressCountry,ArchiveAddress.Country)
         			if nCountrySubscript > 0 then ListSelectPlus(EDT_InvAddressCountry, nCountrySubscript)
         		end
         		
         		
         		if Orders.InvoicingAddressID = Orders.DeliveryAddressID then
         			
         			CBOX_IdenticalInvoicingAddress = 1
         			GR_InvoicingAddress..State = Grayed
         			
         		end
         		
         		
         	else
         		Error("Order not found")
         		return
         	end
         	
         	
         else
         	
         	//Otherwise, try to fill as must was possible, especially with the customer details 
         	if gCustomerUUID <> Nil_UUID then
         		
         		//Fill the customer name beforehand	
         		EDT_Customer = FullCustomerName(gCustomerUUID)
         		
         		//Shipping address, filled with the customer address beforehand
         		HReadSeekFirst(Address,CustomerIDDefaultAddress,[gCustomerUUID,1])
         		if HFound(Address) then
         			
         			EDT_AddressFullName = Address.FullName
         			EDT_CompanyAddress = Address.Company
         			EDT_AddressStreet = Address.Way
         			EDT_AddressComplement = Address.Complement
         			EDT_AddressPostalBox = Address.LocalityPOB
         			EDT_AddressZipCode = Address.ZipCode
         			EDT_AddressCity = Address.City
         			EDT_AddressStateProvince = Address.StateProvince
         			nCountrySubscript is int = ListSeek(COMBO_AddressCountry,Address.Country)
         			if nCountrySubscript > 0 then ListSelectPlus(COMBO_AddressCountry, nCountrySubscript)
         			
         			//Invoicing address, filled with the customer address beforehand
         			EDT_InvAddressFullName = Address.FullName
         			EDT_InvAddressCompany = Address.Company
         			EDT_InvAddressStreet = Address.Way
         			EDT_InvAddressComplement = Address.Complement
         			EDT_InvAddressPostalBox = Address.LocalityPOB
         			EDT_InvAddressZipCode = Address.ZipCode
         			EDT_InvAddressCity = Address.City
         			EDT_InvAddressStateProvince = Address.StateProvince
         			nCountrySubscript = ListSeek(EDT_InvAddressCountry,Address.Country)
         			if nCountrySubscript > 0 then ListSelectPlus(EDT_InvAddressCountry, nCountrySubscript)
         			
         		end
         		
         		CBOX_IdenticalInvoicingAddress = 1
         		GR_InvoicingAddress..State = Grayed
         		
         		
         	end
         	
         end
         
         
         //Management of status: if it is an estimate creation, lock the status combo box
         //Manage a button used to switch from estimate status from order status
         //This change of status is quite important
         ListSelectPlus(COMBO_OrderStatus, gnStatus)
         if gnStatus = ORD_ESTIMATE then
         	
         	BTN_Transfer_in_ord..Visible = True 
         	BTN_Invoice..Visible = False
         	GR_Order_Invoice..Visible = False
         	
         else
         	BTN_Transfer_in_ord..Visible = False
         	
         	//If we are position on an order (and not an estimate)
         	//and if the order is not invoiced yet, propose the invoicing option
         	if gnOrderID <> -1  _and_ Orders.InvoiceNum ~= "" then
         		BTN_Invoice..Visible = True 
         	else
         		BTN_Invoice..Visible = False 
         	end
         	
         	GR_Order_Invoice..Visible = True
         end
         
         
        type : 14
