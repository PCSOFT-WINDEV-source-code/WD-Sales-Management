   -
     name : CalculateTotals
     procedure_id : 2015370097646752986
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      //CalculateTotals ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      Procédure CalculateTotals()
      
      
      rTotalVAT is real
      rTotalBT is real
      rTotalIOT is real
      
      
      // Browse the table rows
      FOR ALL ROW of TABLE_OrdLine
      	
      	// Stores the total BT
      	rTotalBT += TABLE_OrdLine.SET_SubTotalBT
      	// Stores the VAT
      	rTotalVAT +=(TABLE_OrdLine.SET_SubTotalBT*(TABLE_OrdLine.COL_VATRate))
      		
      	
      end
      
      // Calculates the total IOT
      rTotalIOT = rTotalBT + rTotalVAT 
      
      // Enter the totals in the controls
      EDT_TotalBT = rTotalBT
      EDT_TotalVAT = rTotalVAT
      EDT_TotalIOT = rTotalIOT
      
      
      STC_Total = EDT_ShippingCost + EDT_TotalIOT
