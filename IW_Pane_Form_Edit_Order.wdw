                 -
                   name : BTN_Invoice
                   identifier : 0x2bf8086db3d8d4da
                   internal_properties : 
                   properties :
                    x : 65
                    y : 3
                    width : 64
                    height : 70
                   type : 4
                   code_elements :
                    type_code : 4
                    p_codes :
                     -
                       code : |1-
                        //Re-save the order before, in case modifications had been done .. 
                        ExecuteProcess(BTN_Save,trtClick)
                        
                        //Create the invoice
                        sInvoiceNum is string = CreatedInvoice()
                        if sInvoiceNum = "" then
                        	Error("Error! Unable to invoice!")
                        	return
                        end
                        
                        Invoice.CustomerUUID = Orders.CustomerUUID
                        
                        Invoice.OrderNum = Orders.OrderNum
                        Invoice.OrderDate = Orders.OrderDate
                        
                        Invoice.TotalBT = Orders.TotalBT
                        Invoice.TotalVAT = EDT_TotalVAT
                        Invoice.TotalIOT = Orders.TotalIOT
                        Invoice.TotalShippingCost = Orders.ShippingCost
                        
                        //Invoicing address in clear, and normalized
                        Invoice.InvoicingAddress = EDT_InvAddressFullName + [CR] +  EDT_InvAddressComplement +  [CR] + EDT_InvAddressStreet + [CR] + EDT_InvAddressPostalBox + [CR] + EDT_AddressZipCode +  [SPC] +  Upper(EDT_InvAddressCity) + [CR] + Upper(EDT_InvAddressCountry[EDT_InvAddressCountry])
                        
                        //Add the invoice into database and add the invoice number to the corresponding order
                        if HModify(Invoice) then
                        	
                        	Orders.InvoiceNum = sInvoiceNum 
                        	HModify(Orders)
                        	
                        end
                        
                        Notify("Add.Document", Invoice.InvoiceID)	
                        
                        //Close the tab
                        TabClose()
