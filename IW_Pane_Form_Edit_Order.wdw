   -
     name : AssistedInput
     internal_properties : CAAAAAgAAAAVdmAyoKcaL27sxGQvPu2zjiU9NSiiwEeHPHnUqW4sTSxlNdhZPsb18PJOke5uf+rrNA2ifOlSakSVVGBUEpzA4x5z2bZ4UkJ9Akr/e2B5nx4MwkFeYCcgb6Ia08xgpP0JbC7lCS/z3A2+lPjBv8syrRifY+UdBYCA/yxBbolOwPGvXpw45e05wKTUi+nqJbq37WRcLtp0wLOcb7xjv1fweeuYbw==
     procedure_id : 2048777461238069575
     type_code : 14
     code : |1-
      // Summary: Assisted input 
      // Syntax:
      //AssistedInput ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Automatic procedure:
      // The procedure is run manually, during a call in the code, with a time-out set to 30 hundredths of a second
      // 
      
      Procédure AssistedInput()
      
      // If no input, no action
      if EDT_Add_Reference ~= "" _and_ EDT_Add_Caption ~= "" then
      	// Closes the assisted input
      	MyInternalWindow..Plane = 0
      	return
      end
      
      // Searches for the 10 first products that correspond to the input
      QRY_AssistedInputAddProduct.ParamReference = (EDT_Add_Reference ~= "" ? Null else EDT_Add_Reference)
      QRY_AssistedInputAddProduct.ParamVariationCaption = (EDT_Add_Caption ~= "" ? Null else EDT_Add_Caption)
      if HExecuteQuery(QRY_AssistedInputAddProduct) = False then
      	// Closes the assisted input
      	MyInternalWindow..Plane = 0
      	Error("Unable to execute the assisted input request")
      	return
      end
      
      // According to the number of returns
      switch HNbRec(QRY_AssistedInputAddProduct)
      	// If no reference
      	case 0
      		// No action
      		ToastDisplay("No product corresponds to the input", toastShort, vaMiddle, haCenter)
      		ReturnToCapture(EDT_Add_Reference)
      		
      	// 1 result
      	case 1
      		// Reads the only result
      		HReadFirst(QRY_AssistedInputAddProduct)
      		// Stores the information in the edit controls
      		EDT_Add_Reference = QRY_AssistedInputAddProduct.Reference
      		EDT_Add_Caption = QRY_AssistedInputAddProduct.VariationCaption
      		// Closes the assisted input
      		MyInternalWindow..Plane = 0
      		// Positions the input on the add button
      		ReturnToCapture(BTN_Add)
      		
      	// Several results
      	other case
      		
      		// Clears the looper
      		LooperDeleteAll(LOOP_ProductVariation)
      		
      		// Array of product variations, its content will be displayed in the looper 
      		arrListVariations is array of cProductVariation
      		
      		// Browse the query and store the product variations in an array
      		clProductVar is cProductVariation
      		for each QRY_AssistedInputAddProduct 
      			FileToMemory(clProductVar,QRY_AssistedInputAddProduct)
      			ArrayAdd(arrListVariations, clProductVar)
      		end
      		
      		// Displays the list of product variations
      		FOR ALL ELEMENT clProductVar of arrListVariations
      			LooperAddLine(LOOP_ProductVariation,clProductVar.m_sPhoto,clProductVar.m_sReference,clProductVar.m_sVariationCaption, clProductVar.m_rUnitPriceBT,clProductVar.m_bPromotion,clProductVar.m_bNew, clProductVar.m_nProductVariationID)
      		end
      		
      		// Displays the assisted input
      		MyInternalWindow..Plane = 1
      		
      end
