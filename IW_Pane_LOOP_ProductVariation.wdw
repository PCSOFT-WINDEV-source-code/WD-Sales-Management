   -
     name : UPDListProducts
     internal_properties : CAAAAAgAAAC036fyFYRx2hlSudVZ73FPPUWCdX7C1Doi8n6AFIjqapfSCkWJxWMsn/H1F9M2LLIkLKsHkrl+NVgFXtL46RtdDm8ju6HwMwAiQPRxCXP1kwyUCRNCgNzOGKBaLX4WpouoD1D7V4M7yzEbnOzngExR3a76qgFKXjADBb1TjDiMI56k
     procedure_id : 2015370174959000362
     type_code : 14
     code : |1+
      // Summary: Updates the list of products in the window according to the selected filters
      // Syntax:
      //UPDListProducts ([<nMode> is int])
      //
      // Parameters:
      //	nMode (integer - default value=1): Display all or filter the display of products
      // Return value:
      // 	None
      //
      Procdure UPDListProducts(nMode is int = CST_DISPLAYALL)
      
      //Array of product variations, its content will be displayed in the looper 
      arrListVariations is array of cProductVariation
      
      //Clear the looper
      LooperDeleteAll(LOOP_ProductVariation)
      
      
      EDT_Caption = ""
      EDT_Reference = ""
      
      //Configure the query according to the product family selected by the user
      if nMode = CST_DISPLAYALL then
      	
      	//Here, everything must be displayed, no filter on families or characteristics
      	QRY_ProductVariations.ParamProductFamilyID = Null
      	
      else
      	
      	//Find the product variations for this family and for the sub-families of the family 
      	arrFamily is array of int = FamilyGetSubFamilies(TVT_Families.COL_ID)
      	QRY_ProductVariations.ParamProductFamilyID = ArrayToString(arrFamily, TAB)
      	
      end
      
      //Run the query
      HExecuteQuery(QRY_ProductVariations)
      
      //Browse the query and store the product variations in an array
      clProductVar is cProductVariation
      for each QRY_ProductVariations 
      	FileToMemory(clProductVar,QRY_ProductVariations)
      	ArrayAdd(arrListVariations, clProductVar)
      end
      
      //Clear the array according to the filters implemented by the user on the characteristics
      FilterProdVarAccordingToCharacteristics(arrListVariations)
      
      //Display the list of product variations
      FOR ALL ELEMENT clProductVar of arrListVariations
      	LooperAddLine(LOOP_ProductVariation,clProductVar.m_sPhoto,clProductVar.m_sReference,clProductVar.m_sVariationCaption, clProductVar.m_rUnitPriceBT,clProductVar.m_bPromotion,clProductVar.m_bNew, clProductVar.m_nProductVariationID)
      end
      
      
      //Display the number of products 
      stc_NbProducts = StringBuild( "  Total: %1 product(s)", arrListVariations..Count)
     type : 458752
   -
     name : DisplayCharacteristicFilters
     procedure_id : 2015370174959065898
     type_code : 14
     code : |1-
      // Summary: Display the filters
      // Syntax:
      // DisplayCharacteristicFilters ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      Procdure DisplayCharacteristicFilters()
      
      //For each characteristic
      for each Characteristic 
      	
      	//Create a line in the MLZ, it will contain the internal window with the possible values for this characteristic
      	nSubscript is int = mlzAddLine(MZ_FilterCharacteristics)
      	
      	//Inform the internal window of the characteristic that will be managed
      	nHeight is int = MZ_FilterCharacteristics[nSubscript].IWC_Filter.AddCharacteristicValues(Characteristic.CharacteristicID, Characteristic.CharacteristicType)
      	//Adapt the height of the MLZ line to display all the options
      	MZ_FilterCharacteristics[nSubscript]..Height =  nHeight + 20
      	MZ_FilterCharacteristics[nSubscript].IWC_Filter..Height =  nHeight + 20
      	
      end
     type : 458752
   -
     name : FilterProdVarAccordingToCharacteristics
     procedure_id : 2015370174959131434
     type_code : 14
     code : |1+
      // Summary: Clears the list of product variations according to the filters selected by the user
      // Syntax:
      //FilterProdVarAccordingToCharacteristics (<arrListVariations> is array)
      //
      // Parameters:
      //	arrListVariations (array): Array of product variations, modify it directly
      // Return value:
      // 	None
      //
      Procdure FilterProdVarAccordingToCharacteristics(arrListVariations is array of cProductVariation)
      
      clProductVar is cProductVariation
      arrCharatValue is array of int
      
      //For each line found in the MLZ, therefore for each characteristic, 
      for i = 1 _to_ MZ_FilterCharacteristics..Count
      	
      	// asks the internal window that displays the values of this characteristic
      	//to indicate the values selected by the user 
      	arrCharatValue  = MZ_FilterCharacteristics[i].IWC_Filter.GetSelectedValues()
      	
      	//If no filter is implemented this characteristic, there is no need to clear the array 
      	if arrCharatValue..Count > 0 then 
      		
      		//and if a filter is found in this characteristic, browse the array of product variations that we wanted to display
      		//and delete the variations that do not match the criteria
      		for j = arrListVariations..Count to 1 STEP -1
      			
      			clProductVar = arrListVariations[j]
      			
      			//query used to retrieve, for a given product variation and for a given characteristic, the value of this characteristic
      			QRY_CharacteristicsOfVariation.ParamProductVariationID = clProductVar.m_nProductVariationID
      			QRY_CharacteristicsOfVariation.ParamCharacteristicID = MZ_FilterCharacteristics[i].IWC_Filter.GetCharacteristicID()
      			HExecuteQuery(QRY_CharacteristicsOfVariation)
      			HReadFirst(QRY_CharacteristicsOfVariation)
      			//If the characteristic value is among the ones chosen by the user, keep the variation ; otherwise, delete it
      			if ArraySeek(arrCharatValue,asLinear, QRY_CharacteristicsOfVariation.CharacteristicValueID) < 0 then
      				ArrayDelete(arrListVariations,j)
      			end
      			
      		end
      	end
      	
      end
      
     type : 458752
